{
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "plugins": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "ignorePatterns": "/dist",
  "rules": {
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-unused-vars": 0
  }
}
